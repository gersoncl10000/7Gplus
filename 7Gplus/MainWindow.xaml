                        <RowDefinition Height="auto"/>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>
                    <StackPanel  Orientation="Vertical">
                        <StackPanel Orientation="Horizontal">
                            <Button Style="{StaticResource MaterialDesignRaisedLightButton}" Margin="0 12 8 0" Width="100"
                    ToolTip="Resource name: MaterialDesignRaisedLightButton" Click="btnGraficoPPA_Click" >
                                PPA
                            </Button>

                        </StackPanel>
                        <ContentControl x:Name="visorGraficos" Height="Auto"/>
                    </StackPanel>
                </Grid>
