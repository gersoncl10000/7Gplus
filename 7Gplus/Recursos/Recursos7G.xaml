<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:converter ="clr-namespace:_7Gplus.Clases.Converter"
                    xmlns:local="clr-namespace:_7Gplus.re">
    <converter:numberToBrushConverter x:Key="NumberToBrushConverter"/>
    <converter:textToBrushConverter x:Key="TextToBrushConverter"/>
    <converter:callputToBrushConverter x:Key="CallputToBrushConverter"/>


    <Style x:Key="fondoDataGrid" TargetType="{x:Type DataGridCell}">
        <Setter Property="Background" Value="#eceff1"/>
    </Style>



    <Style x:Key="callputCellColordeFondo" TargetType="{x:Type DataGridCell}">
        <Setter Property="Background" Value="{Binding TipoM,
                                            UpdateSourceTrigger=PropertyChanged,
                                            Converter={ StaticResource CallputToBrushConverter }
                                            }"/>
    </Style>
    
